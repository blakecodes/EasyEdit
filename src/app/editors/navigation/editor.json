{
    "name": "navigation",
    "selector": "editor.tinymce",
    "version": 1.0,
    "content": "<div class=\"container-fluid navigation-editor\">\n    <div class=\"row\">\n        <div class=\"col-md-4\">\n            <div class=\"link-visualizer\">\n                <div class=\"control-bar\">\n                    <span>Link Editor</span>\n                    <div class=\"float-right\">\n                        <ul class=\"nav-draggable\">\n                            <li data-type=\"folder\"><img class=\"drag-icon\" src=\"/src/assets/icons/path-finder/folder.png\">\n                                <span class=\"nav-label\">Folder</span>\n                                <i class=\"fa fa-times-circle delete-nav\" aria-hidden=\"true\"></i>\n                                <ul class=\"link-list-sub addNav sub-folder\">\n\n                                </ul>\n                            </li>\n                            <li data-type=\"page\"><img class=\"drag-icon\" src=\"/src/assets/icons/path-finder/page.png\">\n                                <span class=\"nav-label\">Page</span>\n                                <i class=\"fa fa-times-circle delete-nav\" aria-hidden=\"true\"></i>\n                            </li>\n                        </ul>\n                    </div>\n                </div>\n                <div class=\"link-body\">\n                    <ul class=\"link-list\" id=\"link-list\">\n\n                    </ul>\n                </div>\n            </div>\n        </div>\n        <div class=\" col-md-8\">\n            <form action=\"\" id=\"attrForm\">\n                <div class=\"page-nav-editor\">\n                    <div class=\"header\">\n                        Page Attribute Editor\n                    </div>\n                    <div class=\"tabs\">\n                        <div class=\"tab selected\" data-view-tab=\"information\">Information</div>\n                        <div class=\"tab\" data-view-tab=\"security\">Security</div>\n                    </div>\n                    <div class=\"body tab-content\" data-show-tab=\"information\">\n                        <div class=\"row form-group\">\n                            <div class=\"col-md-12\">\n                                <label for=\"\">Page Title</label>\n                                <input type=\"text\" class=\"form-control\" name=\"pageTitle\">\n                                <input type=\"hidden\" class=\"form-control\" name=\"pageIndex\">\n                            </div>\n                        </div>\n                        <div class=\"row form-group\">\n                            <div class=\"col-md-12\">\n                                <label for=\"\">Description</label>\n                                <textarea class=\"form-control md-text\" name=\"pageDescription\"></textarea>\n                            </div>\n                        </div>\n                        <div class=\"row form-group\">\n                            <div class=\"col-md-12\">\n                                <label for=\"\">Page Author</label>\n                                <input type=\"text\" class=\"form-control\" name=\"pageAuthor\">\n                            </div>\n                        </div>\n                        <!-- Check boxes  -->\n                        <div class=\"row form-group\">\n                            <div class=\"col-md-12\">\n                                <div>\n                                    <label>Hidden page? <img src=\"/src/assets/icons/tooltip.png\" class=\"tooltip-ico sp-tooltip\" title=\"Hide this page from the public on your site.\"></label>\n                                </div>\n                                <div class=\"form-check\">\n                                    <label class=\"form-check-label\">\n                                    <input class=\"form-check-input\" type=\"radio\" name=\"hiddenPage\" value=\"hidden\" checked>\n                                    Yes\n                                </label>\n                                </div>\n                                <div class=\"form-check\">\n                                    <label class=\"form-check-label\">\n                                    <input class=\"form-check-input\" type=\"radio\" name=\"hiddenPage\" value=\"unhidden\">\n                                    No\n                                </label>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"row form-group\">\n                            <div class=\"col-md-12\">\n                                <div>\n                                    <label>Hidden from search engines? <img src=\"/src/assets/icons/tooltip.png\" class=\"tooltip-ico sp-tooltip\" title=\"Hide this page from search engine queries.\"></label>\n                                </div>\n                                <div class=\"form-check\">\n                                    <label class=\"form-check-label\">\n                                    <input class=\"form-check-input\" type=\"radio\" name=\"hideSearch\" value=\"hidden\" checked>\n                                    Yes\n                                </label>\n                                </div>\n                                <div class=\"form-check\">\n                                    <label class=\"form-check-label\">\n                                    <input class=\"form-check-input\" type=\"radio\" name=\"hideSearch\" value=\"unhidden\">\n                                    No\n                                </label>\n                                </div>\n                            </div>\n                        </div>\n                        <!-- End check boxes  -->\n                        <div class=\"row form-group\">\n                            <div class=\"col-md-12\">\n                                <label for=\"\">Page Slug <img src=\"/src/assets/icons/tooltip.png\" class=\"tooltip-ico sp-tooltip\" title=\"Set a custom name that will appear in the url box for this page.\"></label>\n                                <div class=\"flex\">\n                                    <span class=\"site-url\">http://yoursite.com/</span>\n                                    <input type=\"text\" class=\"form-control\" name=\"pageUrl\">\n                                    <span>.pml</span>{}\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"body tab-content\" data-show-tab=\"security\" style=\"display: none;\">\n                        <div class=\"row form-group\">\n                            <div class=\"col-md-12\">\n                                <img src=\"/src/assets/icons/locked.png\" class=\"lock-ico\">\n                                <label for=\"\">Members with access</label>\n                                <textarea class=\"form-control md-text\" name=\"pageDescription\"></textarea>\n                                <div class=\"form-link-area\">\n                                    <a href=\"#\">Click here to add all users for viewing access</a>\n                                    <a href=\"#\">Click here to add all groups for viewing access</a>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"row form-group\">\n                            <div class=\"col-md-12\">\n                                <img src=\"/src/assets/icons/locked.png\" class=\"lock-ico\">\n                                <label for=\"\">Contributors with access</label>\n                                <textarea class=\"form-control md-text\" name=\"pageDescription\"></textarea>\n                                <div class=\"form-link-area\">\n                                    <a href=\"#\">Click here to add all users for updating access</a>\n                                    <a href=\"#\">Click here to add all groups for updating access</a>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"editor-bottom\">\n                    <div class=\"row form-group text-center\">\n                        <button type=\"submit\" class=\"btn btn-primary\">Save</button>\n                    </div>\n                </div>\n            </form>\n        </div>\n    </div>\n</div>",
    "scripts": [
        "/src/scripts/editor/navigation.js",
        "/src/app/editors/navigation/editor.js"
    ]
}